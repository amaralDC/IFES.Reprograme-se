<h1 class="text-center my-3">Lista de usuarios</h1>
<table class="table">
	<div class="row">
		<div class="col-12">
			<thead>
				<th>ID do usuario</th>
				<th>Nome</th>
				<th>Email</th>
        <th>Senha</th>
			</thead>
			<tbody>
				{{#each usuario}}
				<tr>
					<th>{{this.id_usuario}}</th>
					<th>{{this.nome}}</th>
					<th>{{this.email}}</th>
          <th>{{this.senha}}</th>
				</tr>
				{{/each}}
			</tbody>
		</div>
	</div>
</table>
